{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "outDir": "js",
    "sourceMap": true,

    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true
  },
  "files": [
    "ts/tower.ts",
    "ts/tower_map.ts"
  ]
}
